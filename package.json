{
  "name": "@mauricioinaz/say-tseltal",
  "version": "1.2.1",
  "description": "Convert integers into Mayan-Tseltal language",
  "author": "Mauricio Gonzalez",
  "repository": {
    "type": "git",
    "url": "https://github.com/mauricioinaz/say-tseltal/"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "jest": "^25.1.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "test": "jest --no-cache ./*",
    "watch": "jest --no-cache --watch ./*"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "es6": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "import/no-unresolved": "off",
      "import/extensions": "off",
      "import/prefer-default-export": "off",
      "import/no-default-export": "off"
    }
  },
  "license": "MIT",
  "keywords": [
    "tseltal",
    "tzeltal",
    "say",
    "numbers_to_text",
    "mauricioinaz"
  ]
}
